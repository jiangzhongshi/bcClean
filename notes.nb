     RowBox[{
      SubscriptBox["W", "ca"], 
      RowBox[{
       SubscriptBox["\[Pi]", "ab"], "(", 
       RowBox[{
        SubsuperscriptBox["y", "b", "2"], "-", 
        RowBox[{"2", 
         SubscriptBox["W", "ca"], 
         SubscriptBox["\[Pi]", "ab"], 
         RowBox[{
